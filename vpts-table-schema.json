{
  "fields": [
    {
      "name": "datetime",
      "description": "Event date as an ISO8601 format string, UTC time",
      "type": "datetime",
      "format": "YYYY-MM-DDThh:mm:ssZ",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "height",
      "description": "Height bin (lower bound) in m above sea level",
      "type": "integer",
      "constraints": {
        "required": true,
        "minimum": 0,
        "maximum": 100000
      }
    },
    {
      "name": "dens",
      "description": "Animal density, in animals/km^3.",
      "type": "number",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    },
    {
      "name": "ff",
      "description": "Horizontal speed, m/s",
      "type": "number",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    },
    {
      "name": "dd",
      "description": "Direction, degrees",
      "type": "number",
      "constraints": {
        "required": true,
        "minimum": 0,
        "maximum": 360
      }
    },
    {
      "name": "eta",
      "description": "Animal reflectivity in cm^2/km^3.",
      "type": "number",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    },
    {
      "name": "sd_vvp",
      "description": "VVP radial velocity standard deviation in m/s",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    },
    {
      "name": "DBZH",
      "description": "Total reflectivity factor (bio + meteo scattering) in dBZ",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": true,
        "minimum": -100,
        "maximum": 100
      }
    },
    {
      "name": "dbz",
      "description": "Animal reflectivity factor in dBZ",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": -100,
        "maximum": 100
      }
    },
    {
      "name": "u",
      "description": "Speed component west to east in m/s",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": -100,
        "maximum": 100
      }
    },
    {
      "name": "v",
      "description": "Speed component north to south in m/s",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": -100,
        "maximum": 100
      }
    },
    {
      "name": "gap",
      "description": "Angular data gap detected in radial velocity data",
      "type": "boolean",
      "format": "default",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "w",
      "description": "Vertical speed component in m/s",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "n_dbz",
      "description": "Number of data points used for reflectivity-based estimates (quantities dbz, eta, dens).",
      "type": "integer",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": 0
      }
    },
    {
      "name": "n",
      "description": "Number of data points used for the ground speed estimates (quantities u, v, w, ff, dd)",
      "type": "integer",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": 0
      }
    },
    {
      "name": "n_all",
      "description": "Number of data points used for the radial velocity standard deviation estimate (quantity sd_vvp)",
      "type": "integer",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": 0
      }
    },
    {
      "name": "n_dbz_all",
      "description": "Number of data points used for the total reflectivity estimate (quantity DBZH)",
      "type": "integer",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": 0
      }
    }
  ]
}
