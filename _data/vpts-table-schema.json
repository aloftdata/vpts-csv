{
  "title": "Fields",
  "fields": [
    {
      "name": "radar",
      "description": "Radar identifier.",
      "type": "string",
      "example": "KBGM",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "datetime",
      "description": "Nominal date and time of the measurement, as an ISO 8601 formatted string in UTC.",
      "type": "datetime",
      "format": "YYYY-MM-DDThh:mm:ssZ",
      "example": "2016-09-01T00:02:00Z",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "height",
      "description": "Lower bound of the altitude bin in m above sea level.",
      "type": "integer",
      "example": "600",
      "constraints": {
        "required": true,
        "minimum": 0,
        "maximum": 10000
      }
    },
    {
      "name": "u",
      "description": "Ground speed component west to east in m/s",
      "type": "number",
      "example": "4.14",
      "constraints": {
        "required": false,
        "minimum": -100,
        "maximum": 100
      }
    },
    {
      "name": "v",
      "description": "Ground speed component north to south in m/s.",
      "type": "number",
      "example": "3.84",
      "constraints": {
        "required": false,
        "minimum": -100,
        "maximum": 100
      }
    },
    {
      "name": "w",
      "description": "Vertical speed in m/s.",
      "type": "number",
      "example": "12.17",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "ff",
      "description": "Horizontal ground speed in m/s.",
      "type": "number",
      "example": "5.65",
      "constraints": {
        "required": "true",
        "minimum": 0
      }
    },
    {
      "name": "dd",
      "description": "Ground speed direction in degrees clockwise from north.",
      "type": "number",
      "example": "47.2",
      "constraints": {
        "required": "true",
        "minimum": 0,
        "maximum": 360
      }
    },
    {
      "name": "sd_vvp",
      "description": "VVP radial velocity standard deviation in m/s.",
      "type": "number",
      "example": "2.8",
      "constraints": {
        "required": "true",
        "minimum": 0
      }
    },
    {
      "name": "gap",
      "description": "Angular data gap detected.",
      "type": "boolean",
      "example": "FALSE",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "eta",
      "description": "Animal reflectivity in cm^2/km^3.",
      "example": "46.9",
      "type": "number",
      "constraints": {
        "required": "true",
        "minimum": 0
      }
    },
    {
      "name": "dens",
      "description": "Animal density in animals/km^3.",
      "type": "number",
      "example": "4.263636363636364",
      "constraints": {
        "required": "true",
        "minimum": 0
      }
    },
    {
      "name": "dbz",
      "description": "Animal reflectivity factor in dBZ.",
      "type": "number",
      "example": "3.36",
      "constraints": {
        "required": false,
        "minimum": -100,
        "maximum": 100
      }
    },
    {
      "name": "dbz_all",
      "description": "Total reflectivity factor (bio + meteo scattering) in dBZ. Sometimes named `DBZH`.",
      "type": "number",
      "example": "0.5",
      "constraints": {
        "required": "true",
        "minimum": -100,
        "maximum": 100
      }
    },
    {
      "name": "n",
      "description": "Number of data points used for the ground speed estimates (quantities `u`, `v`, `w`, `ff`, `dd`).",
      "type": "integer",
      "example": "9006",
      "constraints": {
        "required": false,
        "minimum": 0
      }
    },
    {
      "name": "n_dbz",
      "description": "Number of data points used for reflectivity-based estimates (quantities `dbz`, `eta`, `dens`).",
      "type": "integer",
      "example": "13442",
      "constraints": {
        "required": false,
        "minimum": 0
      }
    },
    {
      "name": "n_all",
      "description": "Number of data points used for the radial velocity standard deviation estimate (quantity `sd_vvp`).",
      "type": "integer",
      "example": "65947",
      "constraints": {
        "required": false,
        "minimum": 0
      }
    },
    {
      "name": "n_dbz_all",
      "description": "Number of data points used for the total reflectivity estimate (quantity `dbz_all`)",
      "type": "integer",
      "example": "104455",
      "constraints": {
        "required": false,
        "minimum": 0
      }
    }
  ]
}
