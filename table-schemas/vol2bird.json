{
  "fields": [
    {
      "name": "date",
      "description": "Nominal date of the volume to which the scan belongs in UTC.",
      "type": "date",
      "format": "%Y%m%d",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "time",
      "description": "Nominal time of the volume to which the scan belongs in UTC.",
      "type": "time",
      "format": "%H%M",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "HGHT",
      "description": "Height bin (lower bound) in m above sea level.",
      "type": "integer",
      "format": "default",
      "constraints": {
        "required": true,
        "minimum": 0,
        "maximum": 100000
      }
    },
    {
      "name": "u",
      "description": "Speed component west to east in m/s.",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": -100,
        "maximum": 100
      }
    },
    {
      "name": "v",
      "description": "Speed component north to south in m/s.",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": -100,
        "maximum": 100
      }
    },
    {
      "name": "w",
      "description": "Vertical speed component (unreliable) in m/s.",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "ff",
      "description": "Horizontal speed in m/s.",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": 0,
        "maximum": 100
      }
    },
    {
      "name": "dd",
      "description": "Direction in degrees clockwise from north.",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": 0,
        "maximum": 360
      }
    },
    {
      "name": "sd_vvp",
      "description": "VVP radial velocity standard deviation in m/s.",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": 0,
        "maximum": 100
      }
    },
    {
      "name": "gap",
      "description": "Angular data gap detected in radial velocity data.",
      "type": "boolean",
      "format": "default",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "dbz",
      "description": "Animal reflectivity factor in dBZ.",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": -100,
        "maximum": 100
      }
    },
    {
      "name": "eta",
      "description": "Animal reflectivity in cm^2/km^3.",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": 0
      }
    },
    {
      "name": "dens",
      "description": "Animal density in animals/km^3.",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": 0
      }
    },
    {
      "name": "DBZH",
      "description": "Total reflectivity factor (bio + meteo scattering) in dBZ.",
      "type": "number",
      "format": "default",
      "constraints": {
        "required": false,
        "minimum": -100,
        "maximum": 100
      }
    },
    {
      "name": "n",
      "description": "Number of data points used for the ground speed estimates (quantities u, v, w, ff, dd).",
      "type": "integer",
      "format": "default",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    },
    {
      "name": "n_dbz",
      "description": "Number of data points used for the radial velocity standard deviation estimate (quantity sd_vvp).",
      "type": "integer",
      "format": "default",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    },
    {
      "name": "n_all",
      "description": "Number of data points used for reflectivity-based estimates (quantities dbz, eta, dens).",
      "type": "integer",
      "format": "default",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    },
    {
      "name": "n_dbz_all",
      "description": "Number of data points used for the total reflectivity estimate (quantity DBZH).",
      "type": "integer",
      "format": "default",
      "constraints": {
        "required": true,
        "minimum": 0
      }
    }
  ],
  "missingValues": [
    "",
    "NaN",
    "nan",
    "-Inf"
  ]
}
